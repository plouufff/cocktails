{
  "name": "plouufff/cocktails",
  "description": "cocktails.plouufff.ovh repository",
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "8.4.16",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/doctrine-bundle": "2.13.3",
    "doctrine/doctrine-migrations-bundle": "3.7.0",
    "doctrine/orm": "2.20.9",
    "doctrine/persistence": "2.5.7",
    "easycorp/easyadmin-bundle": "4.27.5",
    "league/commonmark": "2.8.0",
    "sensio/framework-extra-bundle": "6.2.10",
    "stof/doctrine-extensions-bundle": "1.15.1",
    "symfony/console": "^6.0",
    "symfony/dotenv": "^6.0",
    "symfony/flex": "2.10.0",
    "symfony/framework-bundle": "^6.0",
    "symfony/proxy-manager-bridge": "6.4.28",
    "symfony/runtime": "^6.0",
    "symfony/twig-bundle": "^6.0",
    "symfony/validator": "^6.0",
    "symfony/webpack-encore-bundle": "1.17.2",
    "symfony/yaml": "^6.0",
    "twig/extra-bundle": "3.22.2",
    "twig/markdown-extra": "3.22.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^6.0"
    }
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "7.3.0",
    "doctrine/doctrine-fixtures-bundle": "3.7.3",
    "squizlabs/php_codesniffer": "3.13.5",
    "symfony/browser-kit": "^6.0",
    "symfony/css-selector": "^6.0",
    "symfony/maker-bundle": "1.65.1",
    "symfony/phpunit-bridge": "^6.0",
    "symfony/stopwatch": "^6.0",
    "symfony/web-profiler-bundle": "^6.0"
  }
}
